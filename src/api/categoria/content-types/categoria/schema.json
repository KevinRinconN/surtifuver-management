{
  "kind": "collectionType",
  "collectionName": "categorias",
  "info": {
    "singularName": "categoria",
    "pluralName": "categorias",
    "displayName": "Categoria",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "Nombre"
    },
    "Imagen": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "Color": {
      "type": "enumeration",
      "enum": [
        "Verde",
        "Amarillo",
        "Rojo"
      ]
    },
    "productos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::producto.producto",
      "mappedBy": "categoria"
    },
    "Orden": {
      "type": "integer"
    }
  }
}
